---
import Layout from './Layout.astro';
import { AuthProvider } from '../lib/AuthContext';
import Navbar from '../components/Navbar';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <AuthProvider client:load>
    <Navbar client:load />
    <main>
      <slot />
    </main>
  </AuthProvider>
</Layout>
